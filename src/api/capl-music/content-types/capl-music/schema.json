{
  "kind": "collectionType",
  "collectionName": "capl_musics",
  "info": {
    "singularName": "capl-music",
    "pluralName": "capl-musics",
    "displayName": "Capl-music",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "album": {
      "type": "integer",
      "required": true
    },
    "originMusic": {
      "type": "integer"
    },
    "originAlbum": {
      "type": "integer"
    },
    "artist": {
      "type": "json",
      "required": true
    },
    "cover": {
      "type": "json"
    },
    "composer": {
      "type": "json"
    },
    "lyricist": {
      "type": "json"
    },
    "musicId": {
      "type": "string"
    },
    "videoId": {
      "type": "string"
    },
    "relationArtists": {
      "type": "json"
    },
    "genre": {
      "type": "customField",
      "options": [
        "electronic",
        "dance",
        "hiphop",
        "rock",
        "ballad",
        "fork",
        "soul",
        "trot",
        "kpop",
        "jpop"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "type": {
      "type": "customField",
      "options": [
        "origin",
        "cover",
        "live"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
